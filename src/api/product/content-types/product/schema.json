{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "mrp": {
      "type": "biginteger"
    },
    "images": {
      "type": "string",
      "required": false,
      "regex": "^https?://.*",
      "description": "URL of the product image"
    },
    "sizes": {
      "type": "json",
      "required": false
    },
    "size_prices": {
      "type": "json"
    },
    "add_ons": {
      "type": "json"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "user_carts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::user-cart.user-cart",
      "inversedBy": "products"
    }
  }
}
